#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/ts-jest@29.0.3_hfbqr3rujuziiw7lzul6vua4r4/node_modules/ts-jest/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/ts-jest@29.0.3_hfbqr3rujuziiw7lzul6vua4r4/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/ts-jest@29.0.3_hfbqr3rujuziiw7lzul6vua4r4/node_modules/ts-jest/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/ts-jest@29.0.3_hfbqr3rujuziiw7lzul6vua4r4/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
