#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules/@angular-devkit/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/bin/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules/@angular-devkit/schematics-cli/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules/@angular-devkit/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/@angular-devkit+schematics-cli@15.2.4_chokidar@3.5.3/node_modules:/mnt/e/Projects/NestJS-Auth/server/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/schematics.js" "$@"
else
  exec node  "$basedir/../../bin/schematics.js" "$@"
fi
